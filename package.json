{
  "name": "aws-testing-library",
  "version": "1.0.0",
  "description": "Chai and Jest matchers for aws services",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "JEST_JUNIT_OUTPUT=./reports/junit/${npm_package_name}-test-results.xml jest --runInBand --ci",
    "coverage": "jest --coverage",
    "prettier": "prettier --write src/**/*.ts",
    "prettier:ci": "prettier --list-different src/**/*.ts",
    "tag": "git tag \"v$npm_package_version\" && git push --tags",
    "prepublishOnly": "yarn run tag && yarn lint && yarn run build"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "serverless",
    "testing",
    "aws",
    "jest",
    "chai",
    "mocha",
    "s3",
    "dynamoDb",
    "api",
    "cloudwatch",
    "kinesis",
    "lambda",
    "sqs",
    "sns",
    "step-function"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erezrokah/aws-testing-library.git"
  },
  "homepage": "https://github.com/erezrokah/aws-testing-library/#readme",
  "author": "Erez Rokah",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.2",
    "@types/jest-diff": "^20.0.0",
    "@types/jest-matcher-utils": "^21.0.1",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^10.11.1",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "mockdate": "^2.0.2",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "aws-sdk": "^2.323.0",
    "axios": "^0.18.1",
    "jest-diff": "^23.6.0",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
